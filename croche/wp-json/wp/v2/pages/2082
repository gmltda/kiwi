o = data.getFullYear();\r\n\r\n    return `${dia}\/${mes}\/${ano}`;\r\n}\r\n\r\n\/\/ Atualiza o motivo da promo\u00e7\u00e3o e a data na p\u00e1gina\r\n(function atualizarMotivoEData() {\r\n    const dataHoje = new Date();\r\n    document.getElementById('dataAtual').innerText = formatarData(dataHoje);\r\n})();\r\n<\/script>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-61ca08c elementor-widget__width-initial elementor-widget elementor-widget-image\" data-id=\"61ca08c\" data-element_type=\"widget\" data-widget_type=\"image.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<img decoding=\"async\" src=\"https:\/\/produto-rapido.com\/wp-content\/uploads\/2024\/09\/KIW-removebg-preview.png\" title=\"KIW-removebg-preview\" alt=\"KIW-removebg-preview\" loading=\"lazy\" \/>\t\t\t\t\t\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-8882851 e-flex e-con-boxed e-con e-parent\" data-id=\"8882851\" data-element_type=\"container\" data-settings=\"{&quot;_ha_eqh_enable&quot;:false}\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-6c0ac65 elementor-widget elementor-widget-heading\" data-id=\"6c0ac65\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<h4 class=\"elementor-heading-title elementor-size-default\">Acesse modelos exclusivos como estes e muito mais...<\/h4>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-e60ec61 elementor-widget elementor-widget-heading\" data-id=\"e60ec61\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<p class=\"elementor-heading-title elementor-size-default\">Passe pro lado e veja alguns modelos dispon\u00edveis.<\/p>\t\t<\/div>\n\